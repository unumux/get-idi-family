{
  "name": "@unumux/eslint-config-unumux",
  "description": "",
  "main": "browser.js",
  "scripts": {
    "commit": "git-cz",
    "test": "true",
    "semantic-release": "semantic-release pre && npm publish --access public && semantic-release post"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/unumux/eslint-config-unumux.git"
  },
  "devDependencies": {
    "commitizen": "^2.5.0",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "^1.2.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1"
  },
  "version": "1.1.0"
}